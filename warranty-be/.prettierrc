{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "proseWrap": "never",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "printWidth": 140,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "printWidth": 10
            }
        }
    ]
}
